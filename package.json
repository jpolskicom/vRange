{
  "name": "v-range-component",
  "version": "1.0.7",
  "description": "Vue Range Slider component",
  "keywords": [
    "vue",
    "nuxt",
    "range",
    "plugin",
    "simple",
    "light",
    "slider",
    "range slider",
    "range slider click",
    "range slider touch",
    "range slider input",
    "component"
  ],
  "author": "Jacek Polski",
  "license": "ISC",
  "scripts": {
    "build": "npm run build:umd & npm run build:es & npm run build:unpkg",
    "build:umd": "rollup --config build/rollup.config.js --format umd --file dist/v-range-component.umd.js",
    "build:es": "rollup --config build/rollup.config.js --format es --file dist/v-range-component.esm.js",
    "build:unpkg": "rollup --config build/rollup.config.js --format iife --file dist/v-range-component.min.js"
  },
  "files": [
    "dist/*"
  ],
  "main": "dist/v-range-component.umd.js",
  "module": "dist/v-range-component.esm.js",
  "unpkg": "dist/v-range-component.min.js",
  "browser": {
    "./sfc": "src/v-range-component.vue"
  },
  "devDependencies": {
    "@rollup/plugin-buble": "^0.21.3",
    "@rollup/plugin-commonjs": "^11.1.0",
    "rollup": "^1.17.0",
    "rollup-plugin-postcss": "^4.0.1",
    "rollup-plugin-vue": "^5.0.1",
    "vue": "^2.6.10",
    "vue-template-compiler": "^2.6.10",
    "postcss": "^8.1.9",
    "postcss-import": "12.0.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jpolskicom/vRange.git"
  },
  "bugs": {
    "url": "https://github.com/jpolskicom/vRange/issues"
  },
  "homepage": "https://github.com/jpolskicom/vRange#readme",
  "dependencies": {
    "vue-sfc-rollup": "^4.1.1"
  }
}
